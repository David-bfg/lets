{
  "name": "lets-meteor-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "./node_modules/.bin/eslint . && meteor --settings ./settings/settings-development.json",
    "dev:production": "./node_modules/.bin/eslint . && meteor --production --settings ./settings/settings-production.json",
    "setup:deploy": "cd .deploy && mupx setup",
    "deploy": "cd .deploy && mupx deploy",
    "generate:styles": "et --config src/element-variables.scss --out src/theme",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --settings ./settings/settings-production.json --extra-packages bundle-visualizer"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.51",
    "@cleverbeagle/seeder": "^1.3.1",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^1.0.3",
    "core-js": "^2.5.7",
    "element-theme-chalk": "^2.4.2",
    "element-ui": "^2.4.2",
    "intersection-observer": "^0.4.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "marked": "^0.3.19",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.22.2",
    "simpl-schema": "^1.5.0",
    "vue": "^2.5.16",
    "vue-meteor-tracker": "^1.2.3",
    "vue-router": "^3.0.1",
    "vue-supply": "^0.3.0",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-meteor": "^4.2.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0"
  },
  "meteor": {
    "vueVersion": 2,
    "mainModule": {
      "client": "src/client/index.js",
      "server": "src/server/index.js"
    },
    "testModule": "tests/index.js"
  }
}
